  "type": "module",
  "main": "index.js",
  "bin": {
    "gtfs-export": "bin/gtfs-export.js",
    "gtfs-import": "bin/gtfs-import.js"
  },
  "scripts": {
    "lint": "xo",
    "test": "xo && NODE_ENV=test mocha ./test/mocha/**/*.js --timeout 2000"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/numeric-separators-style": [
        "error",
        {
          "number": {
            "minimumDigits": 9
          }
        }
      ]
    },
    "space": true
  },
  "dependencies": {
    "@turf/helpers": "^6.3.0",
    "better-copy": "^1.0.4",
    "chalk": "^4.1.1",
    "csv-parse": "^4.15.4",
    "csv-stringify": "^5.6.2",
    "lodash-es": "^4.17.21",
    "node-fetch": "^2.6.1",
    "pluralize": "^8.0.0",
    "pretty-error": "^3.0.3",
    "promise-map-series": "^0.3.0",
    "sanitize-filename": "^1.6.3",
    "sqlite": "^4.0.22",
    "sqlite3": "^5.0.2",
    "sqlstring-sqlite": "^0.1.1",
    "strip-bom-stream": "^4.0.0",
    "tmp-promise": "^3.0.2",
    "untildify": "^4.0.0",
    "unzipper": "^0.10.11",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "should": "^13.2.3",
    "xo": "^0.40.1"
  },
  "engines": {
    "node": ">= 12.14.0"
  }
}
